# Database (same as Zephyr app)
DATABASE_URL=postgres://user:pass@host:5432/db
# or
POSTGRES_URL=postgres://user:pass@host:5432/db

# Vercel Blob storage token
BLOB_READ_WRITE_TOKEN=vercel_blob_xxx

# ComfyUI URL (default: http://127.0.0.1:8188)
COMFYUI_URL=http://127.0.0.1:8188

# Workflow path (default: /workspace/ComfyUI/user/default/workflows/workflow_rotate.json)
WORKFLOW_PATH=/workspace/ComfyUI/user/default/workflows/workflow_rotate.json
